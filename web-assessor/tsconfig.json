{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./www",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "angular-air": [
        "libraries/angular-air/"
      ],
      "angular-air/*": [
        "libraries/angular-air/*"
      ],
      "tester1": [
        "dist/tester1"
      ],
      "tester1/*": [
        "dist/tester1/*"
      ],
      "router": [
        "dist/router"
      ],
      "router/*": [
        "dist/router/*"
      ]
    }
  }
}